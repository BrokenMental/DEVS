<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<atomicType host="localhost" modelName="procFinish" xmlns="http://www.saurabh-mittal.com/NewXMLSchema">
    <states>
        <state>waitForJob</state>
        <state>passive</state>
        <state>busy</state>
        <state>finishing</state>
    </states>
    <TimeAdvance>
        <ta>
            <state>waitForJob</state>
            <Timeout>INF</Timeout>
        </ta>
        <ta>
            <state>passive</state>
            <Timeout>0.0</Timeout>
        </ta>
        <ta>
            <state>busy</state>
            <Timeout>10.0</Timeout>
        </ta>
        <ta>
            <state>finishing</state>
            <Timeout>100.0</Timeout>
        </ta>
    </TimeAdvance>
    <LamdaSet>
        <lamda>
            <state>finishing</state>
            <outport>outThatsit</outport>
        </lamda>
        <lamda>
            <state>busy</state>
            <outport>outJob</outport>
        </lamda>
    </LamdaSet>
    <inports>
        <inport>ininStop</inport>
        <inport>ininJob</inport>
    </inports>
    <outports>
        <outport>outThatsit</outport>
        <outport>outJob</outport>
    </outports>
    <deltint>
        <InternalTransition intTransitionID="3">
            <transition>
                <StartState>finishing</StartState>
                <NextState>passive</NextState>
            </transition>
        </InternalTransition>
        <InternalTransition intTransitionID="2">
            <transition>
                <StartState>busy</StartState>
                <NextState>waitForJob</NextState>
            </transition>
        </InternalTransition>
        <InternalTransition intTransitionID="1">
            <transition>
                <StartState>waitForJob</StartState>
                <NextState>waitForJob</NextState>
            </transition>
        </InternalTransition>
    </deltint>
    <deltext>
        <ExternalTransition extTransitionID="3">
            <IncomingMessage>inStop</IncomingMessage>
            <transition>
                <StartState>waitForJob</StartState>
                <NextState>passive</NextState>
            </transition>
            <scheduleIndicator>true</scheduleIndicator>
        </ExternalTransition>
        <ExternalTransition extTransitionID="2">
            <IncomingMessage>inStop</IncomingMessage>
            <transition>
                <StartState>busy</StartState>
                <NextState>finishing</NextState>
            </transition>
            <scheduleIndicator>false</scheduleIndicator>
        </ExternalTransition>
        <ExternalTransition extTransitionID="1">
            <IncomingMessage>inJob</IncomingMessage>
            <transition>
                <StartState>waitForJob</StartState>
                <NextState>busy</NextState>
            </transition>
            <scheduleIndicator>true</scheduleIndicator>
        </ExternalTransition>
    </deltext>
</atomicType>
